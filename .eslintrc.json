{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "only-warn"
    ],

    "settings": {
        "react": {
          "version": "detect"
        }
      },
    "rules": {
        "semi":"off",
        "@typescript-eslint/semi": "off",
	"array-callback-return": ["error", {
		"allowImplicit": false,
		"checkForEach": true,
		"allowVoid": true
	}]
    }
}
